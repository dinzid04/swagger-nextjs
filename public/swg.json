  "paths": {
    "/api/ai/nano-banana": {
      "post": {
        "summary": "Nano Banana AI Image Editor",
        "description": "Edit gambar menggunakan AI Nano Banana dengan prompt teks",
        "tags": [
          "AI Image Editor"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "image": {
                    "type": "string",
                    "format": "binary",
                    "description": "File gambar yang akan di-edit (format: jpg, png, jpeg)"
                  },
                  "prompt": {
                    "type": "string",
                    "description": "Prompt untuk edit gambar (contoh: ubah menjadi anime style)",
                    "example": "ubah gambar ini menjadi anime style"
                  }
                },
                "required": ["image", "prompt"]
              }
            },
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "imageUrl": {
                    "type": "string",
                    "description": "URL gambar eksternal (alternatif dari upload file)",
                    "example": "https://example.com/image.jpg"
                  },
                  "prompt": {
                    "type": "string",
                    "description": "Prompt untuk edit gambar",
                    "example": "ubah gambar ini menjadi anime style"
                  }
                },
                "required": ["prompt"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "success"
                    },
                    "data": {
                      "type": "object",
                      "properties": {
                        "session_id": {
                          "type": "string",
                          "example": "session_123456"
                        },
                        "status": {
                          "type": "string",
                          "example": "succeeded"
                        },
                        "result_url": {
                          "type": "string",
                          "example": "https://nc-cdn.oss-us-west-1.aliyuncs.com/notegpt/web3in1/result.jpg"
                        },
                        "prompt": {
                          "type": "string",
                          "example": "ubah gambar ini menjadi anime style"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad request - parameter tidak lengkap",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "error"
                    },
                    "message": {
                      "type": "string",
                      "example": "Prompt dan gambar diperlukan"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "error"
                    },
                    "message": {
                      "type": "string",
                      "example": "Proses gagal di server"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
