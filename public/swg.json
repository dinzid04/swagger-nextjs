                    "status": {
                      "type": "boolean",
                      "example": false
                    },
                    "message": {
                      "type": "string",
                      "example": "Prompt dan file gambar diperlukan"
                    }
                  }
                },
                "example": {
                  "status": false,
                  "message": "Prompt dan file gambar diperlukan"
