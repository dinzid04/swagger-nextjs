  "/api/cdn": {
    "get": {
      "summary": "CDN API Information",
      "description": "Get information about available CDN providers and their features",
      "tags": ["CDN Services"],
      "responses": {
        "200": {
          "description": "Success",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "status": {
                    "type": "boolean",
                    "example": true
                  },
                  "message": {
                    "type": "string",
                    "example": "CDN Upload API"
                  },
                  "providers": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "name": {
                          "type": "string",
                          "example": "fileio"
                        },
                        "description": {
                          "type": "string",
                          "example": "Simple file sharing with 14 days expiration"
                        },
                        "maxFileSize": {
                          "type": "string",
                          "example": "50MB"
                        },
                        "features": {
                          "type": "array",
                          "items": {
                            "type": "string"
                          },
                          "example": ["14 days expiration", "Any file type", "Simple API"]
                        }
                      }
                    }
                  },
                  "usage": {
                    "type": "object",
                    "properties": {
                      "post": {
                        "type": "string",
                        "example": "Upload file menggunakan multipart/form-data"
                      },
                      "parameters": {
                        "type": "object",
                        "properties": {
                          "file": {
                            "type": "string",
                            "example": "File yang akan diupload"
                          },
                          "provider": {
                            "type": "string",
                            "example": "Preferred CDN provider (optional): fileio, tmpfiles, freeimage, litterbox"
                          }
                        }
                      }
                    }
                  }
                }
              },
              "example": {
                "status": true,
                "message": "CDN Upload API",
                "providers": [
                  {
                    "name": "fileio",
                    "description": "Simple file sharing with 14 days expiration",
                    "maxFileSize": "50MB",
                    "features": ["14 days expiration", "Any file type", "Simple API"]
                  },
                  {
                    "name": "tmpfiles",
                    "description": "Temporary file hosting with 1 hour expiration",
                    "maxFileSize": "50MB",
                    "features": ["1 hour expiration", "Fast upload", "Temporary storage"]
                  },
                  {
                    "name": "freeimage",
                    "description": "Permanent image hosting (images only)",
                    "maxFileSize": "50MB",
                    "features": ["Permanent storage", "Images only", "No registration"]
                  },
                  {
                    "name": "litterbox",
                    "description": "Temporary file storage with 24 hours expiration",
                    "maxFileSize": "50MB",
                    "features": ["24 hours expiration", "Any file type", "Catbox network"]
                  }
                ],
                "usage": {
                  "post": "Upload file menggunakan multipart/form-data",
                  "parameters": {
                    "file": "File yang akan diupload",
                    "provider": "Preferred CDN provider (optional): fileio, tmpfiles, freeimage, litterbox"
                  }
                }
              }
            }
          }
        }
      }
    },
    "post": {
      "summary": "Upload File to CDN",
      "description": "Upload file to CDN and get public URL",
      "tags": ["CDN Services"],
      "requestBody": {
        "required": true,
        "content": {
          "multipart/form-data": {
            "schema": {
              "type": "object",
              "properties": {
                "file": {
                  "type": "string",
                  "format": "binary",
                  "description": "File to upload (max 50MB)"
                },
                "provider": {
                  "type": "string",
                  "description": "Preferred CDN provider",
                  "enum": ["fileio", "tmpfiles", "freeimage", "litterbox"],
                  "example": "fileio"
                }
              },
              "required": ["file"]
            }
          }
        }
      },
      "responses": {
        "200": {
          "description": "Success",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "status": {
                    "type": "boolean",
                    "example": true
                  },
                  "message": {
                    "type": "string",
                    "example": "File uploaded successfully"
                  },
                  "data": {
                    "type": "object",
                    "properties": {
                      "filename": {
                        "type": "string",
                        "example": "image.jpg"
                      },
                      "size": {
                        "type": "number",
                        "example": 1024000
                      },
                      "mimetype": {
                        "type": "string",
                        "example": "image/jpeg"
                      },
                      "url": {
                        "type": "string",
                        "example": "https://file.io/abc123"
                      },
                      "provider": {
                        "type": "string",
                        "example": "file.io"
                      },
                      "expires": {
                        "type": "string",
                        "example": "14 days"
                      },
                      "timestamp": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2024-01-01T00:00:00.000Z"
                      }
                    }
                  }
                }
              },
              "example": {
                "status": true,
                "message": "File uploaded successfully",
                "data": {
                  "filename": "image.jpg",
                  "size": 1024000,
                  "mimetype": "image/jpeg",
                  "url": "https://file.io/abc123",
                  "provider": "file.io",
                  "expires": "14 days",
                  "timestamp": "2024-01-01T00:00:00.000Z"
                }
              }
            }
          }
        },
        "400": {
          "description": "Bad Request",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "status": {
                    "type": "boolean",
                    "example": false
                  },
                  "message": {
                    "type": "string",
                    "example": "File is required"
                  }
                }
              },
              "example": {
                "status": false,
                "message": "File is required"
              }
            }
          }
        },
        "500": {
          "description": "Internal Server Error",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "status": {
                    "type": "boolean",
                    "example": false
                  },
                  "message": {
                    "type": "string",
                    "example": "Upload failed"
                  }
                }
              },
              "example": {
                "status": false,
                "message": "All CDN providers failed"
              }
            }
          }
        }
      }
    }
  },
  
  "/api/downloader/tiktok": {
    "get": {
      "summary": "TikTok Downloader",
      "description": "Download video atau slideshow dari TikTok",
      "tags": ["Downloader Sosmed"],
      "parameters": [
        {
          "name": "url",
          "in": "query",
          "description": "URL TikTok yang akan didownload",
          "required": true,
          "schema": {
            "type": "string",
            "example": "https://vt.tiktok.com/ZSjXNEnbC/"
          }
        }
      ],
      "responses": {
        "200": {
          "description": "Success",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "status": {
                    "type": "boolean",
                    "example": true
                  },
                  "data": {
                    "type": "object",
                    "properties": {
                      "original_url": {
                        "type": "string",
                        "example": "https://vt.tiktok.com/ZSjXNEnbC/"
                      },
                      "type": {
                        "type": "string",
                        "example": "video"
                      },
                      "urls": {
                        "type": "array",
                        "items": {
                          "type": "string"
                        },
                        "example": ["https://example.com/video.mp4"]
                      },
                      "metadata": {
                        "type": "object",
                        "properties": {
                          "title": {
                            "type": "string",
                            "example": "Video Title"
                          },
                          "thumbnail": {
                            "type": "string",
                            "example": "https://example.com/thumb.jpg"
                          },
                          "creator": {
                            "type": "string",
                            "example": "username"
                          }
                        }
                      }
                    }
                  },
                  "timestamp": {
                    "type": "string",
                    "example": "2024-01-01T00:00:00.000Z"
                  }
                }
              }
            }
          }
        },
        "400": {
          "description": "Bad Request",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "status": {
                    "type": "boolean",
                    "example": false
                  },
                  "message": {
                    "type": "string",
                    "example": "URL parameter is required"
                  }
                }
              }
            }
          }
        },
        "500": {
          "description": "Internal Server Error",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "status": {
                    "type": "boolean",
                    "example": false
                  },
                  "message": {
                    "type": "string",
                    "example": "Failed to process TikTok URL"
                  }
                }
              }
            }
          }
        }
      }
    },
